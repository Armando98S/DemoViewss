<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DemoViews.UltEjercicio">
    <ContentPage.Content>
        <StackLayout>
            <TableView Intent="Form">

                <TableSection Title="Primera Secion">
                    <TextCell TextColor="Black" Detail="Departamento">
                    </TextCell>
                    <EntryCell Placeholder="Ingrese Area"
                               Label="Area">


                    </EntryCell>
                    <SwitchCell Text="Provicional"></SwitchCell>
                    <ImageCell ImageSource="cliente.png"></ImageCell>
                </TableSection>

                <TableSection Title="Segunda Secion">
                    <TextCell TextColor="Black" Detail="Profecion">
                    </TextCell>
                    <EntryCell Placeholder="Ingrese Especialidad"
                               Label="Especialidad">


                    </EntryCell>
                    <SwitchCell Text="Guardado"></SwitchCell>
                    <ImageCell ImageSource="fa.png"></ImageCell>
                </TableSection>
            </TableView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>